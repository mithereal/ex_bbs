<div>
  <h2><%= @title %></h2>

  <.form
    let={f}
    for={@changeset}
    id="session_keys-form"
    phx-target={@myself}
    phx-change="validate"
    phx-submit="save">
  
    <%= label f, :key_id %>
    <%= text_input f, :key_id %>
    <%= error_tag f, :key_id %>
  
    <%= label f, :user_id %>
    <%= number_input f, :user_id %>
    <%= error_tag f, :user_id %>
  
    <%= label f, :last_ip %>
    <%= text_input f, :last_ip %>
    <%= error_tag f, :last_ip %>
  
    <%= label f, :last_login %>
    <%= number_input f, :last_login %>
    <%= error_tag f, :last_login %>
  
    <div>
      <%= submit "Save", phx_disable_with: "Saving..." %>
    </div>
  </.form>
</div>
